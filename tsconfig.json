{
  "compilerOptions": {
      "outDir": "./build",
      "module": "CommonJS",
      "target": "ES2019",
      "esModuleInterop": true,
      "allowJs": true,
      "strict": false,
      "baseUrl": ".",
      "paths": {
        "@main/*": ["./src/main/*"],
        "@shared/*": ["./src/shared/*"],
        "@modules/*": ["./src/modules/*"]
      }
  },
}